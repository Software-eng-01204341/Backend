{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/User": {
            "get": {
                "description": "Reads and sends the content of a static data.json file.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "example"
                ],
                "summary": "Get static data",
                "responses": {
                    "200": {
                        "description": "The data from the JSON file",
                        "schema": {
                            "$ref": "#/definitions/example.DataResponse"
                        }
                    },
                    "500": {
                        "description": "Could not read data file.",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "example.DataResponse": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/example.Item"
                    }
                },
                "status": {
                    "type": "string",
                    "example": "success"
                }
            }
        },
        "example.Item": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "name": {
                    "type": "string",
                    "example": "Item A"
                }
            }
        }
    }
}